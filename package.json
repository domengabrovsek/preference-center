{
  "name": "preference-center",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Preference Center",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "migrate:create": "node-pg-migrate create -j sql",
    "migrate:up": "node-pg-migrate up",
    "test": "vitest run tests/api/create-user.test.ts && vitest run tests/api/get-user.test.ts && vitest run tests/api/create-consent-events.test.ts"
  },
  "keywords": [],
  "author": "Domen Gabrovsek",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.10",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4",
    "@eslint/js": "^9.14.0",
    "globals": "^15.12.0",
    "eslint": "^9.14.0",
    "@types/eslint__js": "^8.42.3",
    "typescript-eslint": "^8.13.0",
    "husky": "^9.1.6",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0"
  },
  "dependencies": {
    "@fastify/postgres": "^6.0.1",
    "@fastify/swagger": "^9.2.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@fastify/type-provider-typebox": "^5.0.1",
    "@sinclair/typebox": "^0.33.22",
    "dotenv": "^16.4.5",
    "fastify": "^5.1.0",
    "node-pg-migrate": "^7.7.1",
    "pg": "^8.13.1",
    "vite-tsconfig-paths": "^5.1.2"
  }
}
